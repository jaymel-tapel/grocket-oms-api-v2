{
  "name": "grocket-oms-api-v2",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=local nest start --watch",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=local nest start --debug --watch",
    "start:development": "NODE_ENV=development node dist/src/main",
    "start:prod": "NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:local": "dotenv -e .env.local -- npx prisma migrate dev",
    "migrate:local:create": "dotenv -e .env.local -- npx prisma migrate dev --create-only",
    "migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "migrate:deploy": "dotenv -e .env.production -- npx prisma migrate deploy",
    "migrate:status": "dotenv -e .env.local -- npx prisma migrate status",
    "migrate:status:dev": "dotenv -e .env.development -- npx prisma migrate status",
    "migrate:status:prod": "dotenv -e .env.production -- npx prisma migrate status",
    "prisma:generate": "dotenv -e .env.local -- npx prisma generate",
    "prisma:generate:dev": "dotenv -e .env.development -- npx prisma generate",
    "prisma:generate:prod": "dotenv -e .env.production -- npx prisma generate",
    "studio": "dotenv -e .env.local -- npx prisma studio",
    "studio:dev": "dotenv -e .env.development -- npx prisma studio",
    "studio:prod": "dotenv -e .env.production -- npx prisma studio",
    "prisma:push": "npx prisma db push",
    "prisma:seed": "npx prisma db seed",
    "seed": "ts-node prisma/seed.ts",
    "command": "NODE_ENV=local node -r tsconfig-paths/register ./dist/src/main-cli",
    "command:dev": "NODE_ENV=development node -r tsconfig-paths/register ./dist/src/main-cli",
    "command:prod": "NODE_ENV=production node -r tsconfig-paths/register ./dist/src/main-cli"
  },
