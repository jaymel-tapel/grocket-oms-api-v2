    "passport-local": "^1.0.0",
    "pdfkit": "^0.15.0",
    "prisma-extension-soft-delete": "^1.0.1",
    "prisma-pagination": "^0.2.3",
    "puppeteer": "^22.9.0",
    "puppeteer-html-pdf": "^4.0.8",
    "reflect-metadata": "^0.2.2",
